{
    "name": "tosend/tosend-laravel",
    "description": "Official Laravel SDK for the ToSend email API",
    "type": "library",
    "license": "MIT",
    "keywords": ["email", "api", "tosend", "laravel", "transactional", "mail"],
    "homepage": "https://github.com/tosend/tosend-laravel",
    "support": {
        "issues": "https://github.com/tosend/tosend-laravel/issues",
        "source": "https://github.com/tosend/tosend-laravel"
    },
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/mail": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "ToSend\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ToSend\\Laravel\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "ToSend",
            "email": "support@tosend.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "ToSend\\Laravel\\ToSendServiceProvider"
            ],
            "aliases": {
                "ToSend": "ToSend\\Laravel\\Facades\\ToSend"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
